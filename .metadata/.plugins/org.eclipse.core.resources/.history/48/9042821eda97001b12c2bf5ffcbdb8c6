package Assignment1;

public class HonorsDiploma extends Diploma{

	protected String focus;
	
	public HonorsDiploma(String fn, String ln, String type, String focus) {
		super(fn, ln, type);
		this.focus = focus;
	}
	
	public String toString() {
		String s = fn + " " + ln + " " + type + " " + focus;
		return s;
	}
	
}
